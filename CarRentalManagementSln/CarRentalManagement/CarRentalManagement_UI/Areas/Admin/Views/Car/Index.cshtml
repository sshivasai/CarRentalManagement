<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Available Cars</h2>
		</div>
        <div class="col-6 text-end">
            <a asp-controller="Car" asp-action="Upsert" class="btn btn-primary">
               <i class="bi bi-plus-circle"></i> &nbsp; Add a New Car
			</a>
		</div>
	</div>
    <br /><br />

<table  id="tblData"  class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Model Name</th>
            <th>Brand</th>
            <th>Category</th>
            <th>Year</th>
            <th>Gears</th>
            <th>Transmission</th>
            <th>Condition</th>
            
            <th>Stock</th>

            <th>PricePerDay</th>
            <th>Description</th>
            
            <th></th>
		</tr>
	</thead>
</table>
</div>

@section Scripts{
    <script src="~/js/car.js"></script>
}